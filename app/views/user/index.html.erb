<h1>Users</h1>
<ul>
    <%@users.each do |user|%>
        <li><%=user.first_name%></li>
        <li><%=user.last_name%></li>
        <li><%=user.email%></li>
        <li><%=user.phone_number%></li>
        <li><%= link_to "Delete",
                "/users/#{user.id}",
                method: :delete,
                data: { confirm: "Are you sure?" } %> </li>
        <br>
    <%end%>
</ul>
